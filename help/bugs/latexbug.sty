\renewcommand{\@addtofilelist}[1]{%
% \show\@currnamestack
  \expandafter\latexbug@iftoplevel\@currnamestack {}\@nil
  \begingroup
    \xdef\x{#1}% TODO: one-level sanitize
    \expandafter\ifx\csname latexbug@@\x\endcsname\relax
    \def\y{}\else\xdef\y{\csname latexbug@@\x\endcsname}\fi
    \ifx\y\latexbug@us\else
      \ifx\y\latexbug@ignore\else
         {\let\MessageBreak\relax
         \xdef\zzzz{\zzzz\x \ifx\y\latexbug@empty\else\space\space -> \space \y\fi\MessageBreak}}%
      \fi
    \fi
    \endgroup
  \fi
  \latexbug@addtofilelist{#1}}

